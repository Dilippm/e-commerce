
<%- include('../layout/admin/header.ejs')%>
<%- include('../layout/admin/navbar.ejs')%>
<%- include('../layout/admin/sidebar.ejs')%>


<main id="main" class="main">

  


  

    <section>
        <div class="container mt-4">
            <div class="row">
                <div class="col-md-6">
                    <h2 class="text-center">Edit Category</h2>
                    <form action="" method="post">
                        <label for="">Category</label>
                        <input type="text" name="newcategory" class="form-control" value="<%=vcategory.category%>" required>
    
                         <label for="">Description</label>
                        <input type="text" name="newdescription" class="form-control" value="<%=vcategory.description%>"  required>
    
                         
                         <!-- <label for="">Password</label>
                        <input type="text" name="Password" class="form-control"> -->
                        <button type="submit" class="btn btn-success mt-4">Edit</button> 
                        <% if(typeof message !=="undefined") { %>
                            <h1 style="font-size: 14px;color: brown;"><%=message  %></h1>
                  <%} %>
                    </form>
    
                </div>
            </div>
        </div>
    </section>
    </main><!-- End #main -->
    

<%- include('../layout/admin/footer.ejs')%>